<?xml version="1.0" encoding="utf-8"?>
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="20%"
    android:keyHeight="9%">

    <Row>
        <Key
            android:codes="-2"
            android:keyLabel="abc" />
        <Key
            android:codes="-6"
            android:keyLabel="f(x)" />
        <Key
            android:keyLabel="←"
            android:isRepeatable="true" />
        <Key
            android:keyLabel="→"
            android:isRepeatable="true" />
        <Key
            android:keyLabel="ac" />
        <Key
            android:codes="-5"
            android:keyLabel="c"
            android:isRepeatable="true" />
    </Row>

    <Row>
        <Key
            android:keyLabel="( □ )"
            android:popupKeyboard="@xml/popup_keyboard_main_brackets" />
        <Key
            android:keyLabel="□/□"/>
        <Key
            android:keyLabel="7" />
        <Key
            android:keyLabel="8" />
        <Key
            android:keyLabel="9" />
        <Key
            android:keyLabel="÷" />
    </Row>

    <Row>
        <Key
            android:keyLabel="&lt;"
            android:popupKeyboard="@xml/popup_keyboard_main_less" />
        <Key
            android:keyLabel="&gt;"
            android:popupKeyboard="@xml/popup_keyboard_main_more" />
        <Key
            android:keyLabel="4" />
        <Key
            android:keyLabel="5" />
        <Key
            android:keyLabel="6" />
        <Key
            android:keyLabel="×"/>
    </Row>

    <Row>
        <Key
            android:keyLabel="□²"
            android:popupKeyboard="@xml/popup_keyboard_main_power"
            android:keyEdgeFlags="right" />
        <Key
            android:keyLabel="√□"
            android:popupKeyboard="@xml/popup_keyboard_main_root"
            android:keyEdgeFlags="right" />
        <Key
            android:keyLabel="1" />
        <Key
            android:keyLabel="2" />
        <Key
            android:keyLabel="3" />
        <Key
            android:keyLabel="-" />
    </Row>

    <Row>
        <Key
            android:keyLabel="x"
            android:popupKeyboard="@xml/popup_keyboard_main_xyz" />
        <Key
            android:keyLabel="%" />
        <Key
            android:keyLabel="0" />
        <Key
            android:keyLabel="." />
        <Key
            android:keyLabel="=" />
        <Key
            android:keyLabel="+" />
    </Row>

</Keyboard>
